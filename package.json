{
  "private": true,
  "dependencies": {
    "react": "^0.14.2"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "ecstatic": "^1.2.0",
    "function-bind": "^1.0.2",
    "phantomjs": "^1.9.18",
    "react-dom": "^0.14.2",
    "tape": "^4.2.2",
    "watchify": "^3.6.0",
    "webdriverio": "^3.2.6"
  },
  "scripts": {
    "build": "browserify test/harness.js > public/browser.js",
    "phantom": "phantomjs --local-to-remote-url-access=yes --ignore-ssl-errors=yes --ssl-protocol=any --webdriver=4444",
    "start": "ecstatic --cache 0 public",
    "test": "tape test/*.test.js",
    "watch": "watchify test/harness.js -o public/browser.js -dv"
  }
}
